{
  "projectName": "gemini-scrapper",
  "outputPath": "gemini-scrapper-merged.txt",
  "port": 9876,
  "useGitignore": true,
  "onUpsertCommand": "",
  "ignorePatterns": [
    "node_modules",
    "node_modules*.log",
    "package-lock.json",
    "yarn.lock",
    "pnpm-lock.yaml",
    "codemerge.json",
    "merged-output.txt",
    ".env",
    ".env.*",
    "**/.DS_Store",
    "coverage",
    "coverage*.ts",
    "***.tsx",
    "**/*.md"
  ],
    "includePatterns": [
    "**/*.ts",
    "**/*.js",
    "**/*.tsx",
    "**/*.jsx",
    "**/*.json",
    "**/*.md",
    "Dockerfile"
  ]
}
